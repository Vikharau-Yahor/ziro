{
  "name": "React.Sample.Webpack.CoreMvc",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build-web": "webpack",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./wwwroot/react-dev --include-path ./node_modules wwwroot/react-dev/ -o wwwroot/react-dev/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./wwwroot/react-dev --include-path ./node_modules wwwroot/react-dev/ -o wwwroot/react-dev/ --watch --recursive",
    "start-js": "react-scripts start --include-path ./wwwroot",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.5",
    "babel-runtime": "6.26.0",
    "style-loader": "0.23.1",
    "css-loader": "2.1.1",
    "image-webpack-loader": "4.6.0",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
